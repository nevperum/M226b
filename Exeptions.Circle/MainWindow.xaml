<Window x:Class="Exeptions.Circle.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Exeptions.Circle"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnException1" Content="Ohne Fehlerbehebung" HorizontalAlignment="Left" Height="36" Margin="88,73,0,0" VerticalAlignment="Top" Width="583" Click="btnException1_Click"/>
        <Button x:Name="btnException2" Content="Beispiel Zahl Konvertieren " HorizontalAlignment="Left" Height="37" Margin="88,262,0,0" VerticalAlignment="Top" Width="583" Click="btnException2_Click"/>
        <Button x:Name="btnException3" Content="Mit Fehlerbehebung" HorizontalAlignment="Left" Height="36" Margin="88,135,0,0" VerticalAlignment="Top" Width="583" Click="Button_Click"/>
        <Button x:Name="btnException4" Content="Beispiel Circle" HorizontalAlignment="Left" Height="36" Margin="88,321,0,0" VerticalAlignment="Top" Width="583" Click="btnException4_Click"/>
        <Button x:Name="btnException5" Content="Behandlung mehrere Fehler" HorizontalAlignment="Left" Height="36" Margin="88,193,0,0" VerticalAlignment="Top" Width="583" Click="btnException5_Click"/>

    </Grid>
</Window>
